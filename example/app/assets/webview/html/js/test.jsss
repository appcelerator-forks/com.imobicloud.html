// default click event for link + tags
// add classname touchDisabled to disable click event on tag
// $('#app').on('click', UTILS.onClick);

function loadList(list) {
	var html = [];
	
	html.push('<ul>');
	for(var i=0,j=list.length; i<j; i++){
	  	var item = list[i];
	  	html.push('<li data-id="' + item.id + '">' + item.title + '</li>');
	};
	html.push('</ul>');
	
  	$('#app').html( html.join('') );
  	$('#app').on('click', function(e) {
  		var data = $(e.target).data();
  		data && UTILS.fireEvent(e, 'listClick', data);
  	});
  	
  	UTILS.hideAI();
}

